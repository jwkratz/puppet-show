{
  "launch": {
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--no-first-run",
      "--no-zygote",
      "--single-process",
      "--disable-gpu"
    ],
    "headless": false
  },
  "page": {
    "request": {
      "blockedTypes": [
        "image",
        "media",
        "font",
        "texttrack",
        "object",
        "beacon",
        "csp_report",
        "imageset"
      ],
      "blockedUrls": [
        "quantserve",
        "adzerk",
        "doubleclick",
        "adition",
        "exelator",
        "sharethrough",
        "cdn.api.twitter",
        "google-analytics",
        "googletagmanager",
        "google",
        "fontawesome",
        "facebook",
        "analytics",
        "optimizely",
        "clicktale",
        "mixpanel",
        "zedo",
        "clicksor",
        "tiqcdn"
      ]
    },
    "goto": {
      "options": {
        "waitUntil": "networkidle2"
      }
    }
  }
}
